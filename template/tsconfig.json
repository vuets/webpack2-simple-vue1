{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "noLib": false,
        "sourceMap": false,
        "declaration": false,
        "noImplicitAny": false,
        "preserveConstEnums": false,
        "emitDecoratorMetadata": false,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "removeComments": true
    },
    "compileOnSave": false,
    "files": [
        "./src/main.ts"
    ]
}
